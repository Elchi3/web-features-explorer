{% if feature.mdnUrls.length > 0 %}
  <div class="mdn-docs">
    <h3>MDN docs</h3>
    <ul class="link-list">
      {% for url in feature.mdnUrls %}
        <li class="link-list-item">
          <a href="{{ url.url }}">{{ url.title }}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
{% endif %}